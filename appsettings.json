{
  "ConnectionStrings": {
    "Redis": "localhost",
    "RedisDefaultDb": "0",
    "RedisInstance": "Logging"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\TracingRedis\\log-.txt",
          "restrictedToMinimumLevel": "Information",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": null
        }
      }
    ]
  }
}